{
  "name": "msf-ai-assistant",
  "version": "2.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-http-header-size=32768' next dev",
    "build": "NODE_OPTIONS='--max-http-header-size=32768' next build",
    "start": "NODE_OPTIONS='--max-http-header-size=32768' next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test:node": "vitest --run --config ./vitest.config.node.mts",
    "test:jsdom": "vitest --run --config ./vitest.config.jsdom.mts",
    "test": "npm run test:node && npm run test:jsdom",
    "coverage:node": "vitest run --coverage --config ./vitest.config.node.mts",
    "coverage:jsdom": "vitest run --coverage --config ./vitest.config.jsdom.mts",
    "coverage": "npm run coverage:node && npm run coverage:jsdom",
    "search:config": "npx ts-node --project tsconfig.scripts.json scripts/search/index.ts",
    "search:config-downtime": "npx ts-node --project tsconfig.scripts.json scripts/search/index.ts --allow-downtime",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/azure": "^2.0.54",
    "@ai-sdk/openai": "^2.0.53",
    "@ai-sdk/react": "^2.0.76",
    "@azure/ai-agents": "^1.1.0",
    "@azure/ai-projects": "^1.0.0-beta.8",
    "@azure/identity": "^4.4.1",
    "@azure/monitor-ingestion": "^1.1.0",
    "@azure/openai": "^2.0.0-beta.2",
    "@azure/search-documents": "^12.1.0",
    "@azure/storage-blob": "^12.23.0",
    "@azure/storage-queue": "^12.24.0",
    "@dqbd/tiktoken": "^1.0.2",
    "@launchdarkly/node-server-sdk": "^9.7.0",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-virtual": "^3.13.12",
    "@testing-library/user-event": "^14.6.1",
    "ai": "^5.0.76",
    "cheerio": "^1.0.0",
    "eventsource-parser": "^0.1.0",
    "form-data": "^4.0.0",
    "ipaddr.js": "^2.1.0",
    "katex": "^0.16.25",
    "launchdarkly-react-client-sdk": "^3.6.1",
    "microsoft-cognitiveservices-speech-sdk": "^1.40.0",
    "mime-types": "^2.1.35",
    "next": "^15.2.4",
    "next-auth": "^5.0.0-beta.25",
    "next-intl": "^4.3.11",
    "next-pwa": "^5.6.0",
    "openai": "^4.96.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "react-swipeable": "^7.0.2",
    "sharp": "^0.32.6",
    "streamdown": "^1.4.0",
    "uuid": "^9.0.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@secretlint/quick-start": "^11.2.5",
    "@secretlint/secretlint-rule-preset-recommend": "^11.2.5",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jsdom": "^21.1.1",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^24.9.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.22.0",
    "eslint-config-next": "^15.2.4",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^16.2.4",
    "postcss": "^8.4.31",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "*": [
      "secretlint"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  }
}
