{
  "name": "msf-ai-assistant",
  "version": "2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test:node": "vitest --run --config ./vitest.config.node.ts",
    "test:jsdom": "vitest --run --config ./vitest.config.jsdom.ts",
    "test": "npm run test:node && npm run test:jsdom",
    "coverage:node": "vitest run --coverage --config ./vitest.config.node.ts",
    "coverage:jsdom": "vitest run --coverage --config ./vitest.config.jsdom.ts",
    "coverage": "npm run coverage:node && npm run coverage:jsdom",
    "search:config": "npx ts-node --project tsconfig.scripts.json scripts/search/index.ts",
    "search:config-downtime": "npx ts-node --project tsconfig.scripts.json scripts/search/index.ts --allow-downtime"
  },
